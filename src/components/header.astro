---
const siteDomein = "https://tonkotsuya-zun.com/system_panel/uploads/images/";
---

<header id="jsHeader" class="header">
    <div class="headerInr">
        <a href="./" class="logo">
            <img src={siteDomein + '20240705165230486654.png'} alt="とんこつ家ZUN ロゴ画像" width="248" height="67" class="logoImg" />
        </a>
        <nav class="headerNav">
            <div class="links">
                <a href="./commitment" class="navLink">
                    <p class="navLinkTxt">こだわり</p>
                </a>
                <a href="./menu" class="navLink">
                    <p class="navLinkTxt">メニュー</p>
                </a>
                <a href="./drink" class="navLink">
                    <p class="navLinkTxt">ドリンク</p>
                </a>
                <a href="./recruit" class="navLink">
                    <p class="navLinkTxt">採用情報</p>
                </a>
                <a href="./shop" class="navLink">
                    <p class="navLinkTxt">店舗情報</p>
                </a>
                <a href="#" class="navLink">
                    <p class="navLinkTxt">新着情報</p>
                </a>
                <div class="tel">
                    <a href="tel:0263878839" class="telLink link">
                        <img src={siteDomein + '20240705222049522701.png'} alt="電話アイコン画像" width="24" height="24" />
                        <p class="telLinkTxt">0263-87-8839</p>
                    </a>
                    <p class="hours">OPEN / 11:30〜26:00(不定休)</p>
                </div>
            </div>
            <div class="btns">
                <a href="#" target="_blank" rel="noopener" class="icon link">
                    <img src={siteDomein + '20240705222049868598.png'} alt="Instagramアイコン画像" width="24" height="24" />
                </a>
                <a href="./contact" class="normalBtn contactBtn">
                    <div class="contactBtnIcon">
                        <img src={siteDomein + '20240705222049526634.png'} alt="メールアイコン画像" width="24" height="24" class="defaultImg">
                        <img src={siteDomein + '20240708163837487331.png'} alt="メールアイコン画像" width="24" height="24" class="hoverImg">
                    </div>
                    <p class="contactBtnTxt">お問い合わせ</p>
                </a>
            </div>
        </nav>
    </div>
    <div class="hamburger">
        <p class="span"></p>
